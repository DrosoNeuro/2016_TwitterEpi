package user_interface;

/**
 *
 * @author toddbodnar
 */
public class TwitterMessage extends Throwable{
    public TwitterMessage()
    {
         super();
    }
    
    public TwitterMessage(String s)
    {
        super("Twitter Exception: "+s);
    }
    
    String error;
}

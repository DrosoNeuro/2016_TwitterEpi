      protected void loadPCA(File pcas) {
        PCA_weights = new double[keywords.length][pcaCount];
        try {
            BufferedReader br = new BufferedReader(new FileReader(pcas));
            String line;
            int keyword=0;
            while ((line = br.readLine()) != null) {
                String split[] = line.split(",");
                for(int ct=0;ct<pcaCount;ct++)
                    PCA_weights[keyword][ct] = Double.parseDouble(split[ct+1]);
                
                keyword++;
            }
            
            
            br.close();
        } catch (IOException ex) {
            Logger.getLogger(userClassifierBinary.class.getName()).log(Level.SEVERE, null, ex);
        }
    }

    static double PCA_weights[][];
    weka.classifiers.Classifier classifier;
    static int pcaCount=75;
